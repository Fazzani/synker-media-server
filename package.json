{
  "name": "holo-media-server",
  "author": "Heni Fazzani",
  "private": true,
  "version": "0.0.1",
  "description": "HOLO Media server",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "postinstall": "cp node_modules/font-awesome/fonts/* public/fonts && cp node_modules/font-awesome/css/* public/css",
    "dev": "nodemon",
    "start": "node ./dist/app.js",
    "prod": "set NODE_ENV=production && npm run build && npm run start",
    "lint": "eslint server",
    "test": "echo \"No test in this example\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fazzani/synker-media-server.git"
  },
  "keywords": [
    "rtmp",
    "flvjs",
    "hls",
    "streaming",
    "server",
    "synker",
    "nodejs"
  ],
  "dependencies": {
    "@silvermine/videojs-chromecast": "^1.0.1",
    "@videojs/http-streaming": "^1.2.1",
    "chalk": "^2.4.1",
    "circular-json": "^0.5.5",
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "fluent-ffmpeg": "^2.1.2",
    "flv.js": "^1.4.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "node-media-server": "^1.4.3",
    "socket.io": "2.0.2",
    "video.js": "^7.1.0",
    "videojs-chromecast": "^2.0.8",
    "videojs-contrib-dash": "^2.10.0",
    "videojs-contrib-hls": "^5.14.1",
    "videojs-contrib-quality-levels": "^2.0.6",
    "videojs-flash": "^2.1.1",
    "videojs-hlsjs": "^1.4.8",
    "videojs-overlay": "^2.1.2",
    "videojs-playlist": "^4.2.3",
    "videojs-playlist-ui": "^3.4.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.1.2",
    "@types/circular-json": "^0.4.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/jquery": "^3.3.5",
    "@types/node": "^10.5.6",
    "@types/socket.io": "^1.4.36",
    "eslint": "3.0.1",
    "eslint-config-standard": "5.3.1",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-standard": "1.3.2",
    "nodemon": "1.18.3",
    "ts-node": "^7.0.0",
    "typescript": "2.8.4"
  }
}
